{
    "label": "AnyLog Node Storage",
    "description": "Pattern for deploying AnyLog Node Storage service",
    "services": [
        {
            "serviceUrl": "https://github.com/AnyLog-co/deployment-scripts.git",
            "serviceOrgid": "anylogco",
            "serviceArch": "amd64",
            "serviceVersions": [
                {
                    "version": "1.1",
                    "variables": {
                        "LICENSE_KEY": "",
                        "NODE_TYPE": "master",
                        "NODE_NAME": "anylog-master",
                        "COMPANY_NAME": "New Company",
                        "ANYLOG_SERVER_PORT": "32048",
                        "ANYLOG_REST_PORT": "32049",
                        "TCP_BIND": "false",
                        "REST_BIND": "false",
                        "DB_TYPE": "sqlite",
                        "DB_USER": "",
                        "DB_PASSWD": "",
                        "DB_IP": "127.0.0.1",
                        "DB_PORT": "5432",
                        "LEDGER_CONN": "127.0.0.1:32048",
                        "MONITOR_NODES": "false"
                    }
                }
            ]
        }
    ]
}